{
  "version": 2,
  "builds": [
    {
      "source": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "source": "/api/weather",
      "destination": "backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ]
}

