{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node",
        "regions": ["syd1"]  
      }
    ],
    "routes": [
      {
        "src": "/api/weather",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/server.js"
      }
    ]
  }
  
  